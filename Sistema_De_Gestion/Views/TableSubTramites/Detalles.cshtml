@model TableSubTramite
<div>
    <h4>Detalles Tramite: @Model.TituloSubTramite </h4>
    <hr />
    <br />
    <dl class="row">
        <dt class="col-sm-10">
            Descripcion: 
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.DescripcionSubTramite)
        </dd>
        <dt class="col-sm-10">
            Dado de Alta el:
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.FechaAlta)
        </dd>
        <dt class="col-sm-10">
            Ultima Modificacion:
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.FechaUltModificacion)
        </dd>
        <dt class="col-sm-10">
            @Html.DisplayNameFor(model => model.Habilitado)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Habilitado)
        </dd>
    </dl>
</div>
<div>
    <a class="btn btn-primary" asp-action="Edit" asp-route-id="@Model.IdsubTramite">Editar</a> |
    <a class="btn btn-secondary" asp-action="Index">Listado de Tramites</a>
</div>
